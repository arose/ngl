

stage.loadFile( __dir__ + "1blu.pdb", function( o ){

    o.addRepresentation( "cartoon" );
    o.addRepresentation( "licorice", "sidechainAttached and not( SF4 or CYS )" )
        .setVisibility( false );
    o.addRepresentation( "hyperball", "sidechainAttached and ( SF4 or CYS )" );
    o.centerView();

    o.setName( "1BLU" );

} );

